<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:drawable/editbox_dropdown_light_frame">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@android:drawable/alert_light_frame">
        <TextView
            android:text="INVOICE NO"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/lbinvno"
            android:textSize="24sp"
            android:textColor="@android:color/black"
            android:paddingLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/txtInvnp"
            android:editable="false"
            android:textColor="#ff140000"
            android:paddingLeft="5dp"
            android:background="@android:drawable/edit_text" />
        <TextView
            android:text="CUSTOMER"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/lbinvno"
            android:paddingLeft="5dp"
            android:textSize="24sp"
            android:textColor="@android:color/black" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/txtInvcust"
            android:editable="false"
            android:paddingLeft="5dp"
            android:textColor="#ff140000"
            android:background="@android:drawable/edit_text" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@android:drawable/alert_light_frame">
        <TextView
            android:text="ITEM CODE"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#ff093bf6"
            android:paddingLeft="5dp"
            android:textSize="24sp"
            android:id="@+id/textView1" />
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal"
            android:weightSum="3">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2.5"
                android:padding="2dip">
                <Spinner
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtcode"
                    android:paddingLeft="5dp"
                    android:textSize="24sp"
                    android:background="@android:drawable/spinner_dropdown_background"
                    android:fitsSystemWindows="false" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="0.5"
                android:gravity="center"
                android:paddingLeft="5dip">
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:id="@+id/newinv_bfind"
                    android:background="@android:drawable/ic_search_category_default" />
            </LinearLayout>
        </LinearLayout>
        <TableLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tableLayout1">
            <TableRow
                android:id="@+id/tableRow1">
                <TextView
                    android:text="QUANTITY"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#fff60935"
                    android:paddingLeft="5dp"
                    android:textSize="24sp"
                    android:id="@+id/textView4"
                    android:width="120dp" />
                <TextView
                    android:text="UNIT PTICE"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#ff093bf6"
                    android:paddingLeft="5dp"
                    android:textSize="24sp"
                    android:id="@+id/textView5"
                    android:width="140dp" />
                <TextView
                    android:text="TAX"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#ff093bf6"
                    android:paddingLeft="5dp"
                    android:textSize="24sp"
                    android:id="@+id/textView5"
                    android:width="100dp" />
                <TextView
                    android:text="AMOUNT"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#ff093bf6"
                    android:paddingLeft="5dp"
                    android:textSize="24sp"
                    android:id="@+id/textView5"
                    android:width="150dp" />
            </TableRow>
            <TableRow
                android:id="@+id/tableRow1">
                <EditText
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtqty"
                    android:textSize="24sp"
                    android:paddingLeft="5dp"
                    android:textColor="#ff140000"
                    android:background="@android:drawable/edit_text" />
                <EditText
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtprice"
                    android:textSize="24sp"
                    android:textColor="#ff140000"
                    android:paddingLeft="5dp"
                    android:background="@android:drawable/edit_text"
                    android:editable="false" />
                <EditText
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/txttaxamt"
                    android:textColor="#ff140000"
                    android:textSize="24sp"
                    android:paddingLeft="5dp"
                    android:background="@android:drawable/edit_text"
                    android:editable="false" />
                <EditText
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtamount"
                    android:textSize="24sp"
                    android:textColor="#ff140000"
                    android:paddingLeft="5dp"
                    android:background="@android:drawable/edit_text"
                    android:editable="false" />
                <TextView
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/txttax"
                    android:textColor="#ff140000"
                    android:paddingLeft="5dp"
                    android:textSize="24sp"
                    android:background="@android:drawable/edit_text"
                    android:selectAllOnFocus="false"
                    android:editable="false"
                    android:width="60dp" />
            </TableRow>
        </TableLayout>
    </LinearLayout>
    <TableLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableLayout1">
        <TableRow
            android:id="@+id/tableRow1">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff"
                android:typeface="sans"
                android:id="@+id/Save"
                android:drawableLeft="@drawable/add"
                android:width="140dp"
                android:text="ADD" />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff"
                android:typeface="sans"
                android:id="@+id/Cancel"
                android:drawableLeft="@drawable/left"
                android:width="140dp"
                android:text="CANCEL" />
        </TableRow>
        <TableRow
            android:id="@+id/tableRow2">
        </TableRow>
    </TableLayout>
</LinearLayout>